import java.util.Scanner;
class palin
{
            public static void main(String[] args)
            {
                Scanner Obj = new Scanner(System.in);
                int num=Obj.nextInt();
                int rev = 0;
                int org=num;
                while(num!=0)
                {   
                    int digit=num%10;
                    num=num/10;
                    rev=rev*10+digit;
                }
                if(org==rev)
                        System.out.println("palindrome");
                else
                        System.out.println("not palindrome");
            }
}
